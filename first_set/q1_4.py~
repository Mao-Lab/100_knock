# coding: utf-8
import sys

#argvs = sys.argv

def open_txt(file_name):
    f = open(file_name)
    line = f.readlines()
    f.close()
    return line

f1 = open_txt('col1.txt')
f2 = open_txt('col2.txt')

i = 0
for line in f1:
    print line.rstrip() + '\t' + f2[i].rstrip()
    i = i + 1

